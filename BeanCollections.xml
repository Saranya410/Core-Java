<?xml version = "1.0" encoding = "UTF-8"?>

<beans xmlns = "http://www.springframework.org/schema/beans"
   xmlns:xsi = "http://www.w3.org/2001/XMLSchema-instance"
   xsi:schemaLocation = "http://www.springframework.org/schema/beans
   http://www.springframework.org/schema/beans/spring-beans-3.0.xsd">

   <bean id = "c" class = "com.deloitte.CollectionsExample">
      <property name = "cityList">
      <list>
      <value>Bang</value>
      <value>cbe</value>
      <value>mad</value>
      <value>tuti</value>
      </list>
      </property>
     
      <property name = "citySet">
    <set>
      <value>Bang</value>
      <value>cbe</value>
      <value>mad</value>
      <value>tuti</value>
      </set>
      </property>
      
       <property name = "cityMap">
       <map>
       <entry key="1" value="bang"></entry>
        <entry key="2" value="cbe"></entry>
         <entry key="3" value="mad"></entry>
          <entry key="4" value="tuti"></entry>
       
       
       
       </map>
       </property>
      
         <property name = "cityProp"> 
         <props>
         <prop key="one">Bang</prop>
         <prop key="two">cbe</prop>
         <prop key="three">mad</prop>
         <prop key="four">tuti</prop>
         
         </props>
         
   </property>
  </bean>

</beans> 	