<?xml version = "1.0" encoding = "UTF-8"?>

<beans xmlns = "http://www.springframework.org/schema/beans"
   xmlns:xsi = "http://www.w3.org/2001/XMLSchema-instance"
   xsi:schemaLocation = "http://www.springframework.org/schema/beans
   http://www.springframework.org/schema/beans/spring-beans-3.0.xsd">

   <bean id = "hw" class = "com.deloitte.HelloWorld">
      <property name = "msg1" value = "Hi msg1"/>
      <property name = "msg2" value = "Hi msg2"/>
   
  </bean>    
      <bean id = "hi" class = "com.deloitte.HelloIndia" parent="hw">
      
       <property name = "msg1" value = "Hello msg1"/>
        <property name = "msg3" value = "Hello msg3"/>
       
       
       
   </bean>

</beans> 	